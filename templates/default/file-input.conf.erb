# <%= @tag %>.conf - Syslog file inputs for <%= @tag %>
#
# Auto-generated by Chef.
# Local modifications will be overwritten.
#
input(type="imfile"
      File="<%= @file_name %>"
      Tag="<%= @tag %>:"
<% if @severity %>
      Severity="<%= @severity %>"
<% end %>
<% if @facility %>
      Facility="<%= @facility %>"
<% end %>
<% if @state_file %>
      StateFile="<%= @state_file %>"
<% end %>
<% @input_parameters.each do |key,value| -%>
      <%= "#{key}=\"#{value}\"" %>
<% end %>
)
